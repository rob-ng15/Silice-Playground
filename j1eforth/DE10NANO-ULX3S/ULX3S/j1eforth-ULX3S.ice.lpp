                x = v_gpu_x;
                y = v_gpu_y;
                gpu_active_colour = v_gpu_colour;
                param0 = v_gpu_param0;
                param1 = v_gpu_param1;
                param2 = v_gpu_param2;
                param3 = v_gpu_param3;
                write = v_gpu_write;
