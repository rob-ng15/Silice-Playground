    input   int16 gpu_param0,
    input   int16 gpu_param1,
    input   int16 gpu_param2,
    input   int16 gpu_param3,
    input   uint4 gpu_write,

    // From VECTOR DRAWER
    input   int11 v_gpu_x,
    input   int11 v_gpu_y,
    input   uint7 v_gpu_colour,
    input   int11 v_gpu_param0,
    input   int11 v_gpu_param1,
