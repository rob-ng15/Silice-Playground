        if( dl_gpu_write != 0 ) {
            x = dl_gpu_x;
            y = dl_gpu_y;
            gpu_active_colour = dl_gpu_colour;
            param0 = dl_gpu_param0;
            param1 = dl_gpu_param1;
            param2 = dl_gpu_param2;
            param3 = dl_gpu_param3;
            write = dl_gpu_write;
        } else {
